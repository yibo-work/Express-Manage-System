<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.OrderListDao">

    <select id="findById" resultType="com.model.OrderList">
        select `id`,
               `order_no`,
               `user_id`,
               `goods`,
               `username`,
               `phone`,
               `start_express`,
               `end_express`,
               `arrive_express`,
               `status`,
               `create_time`
        from order_list
        where id = #{id}
    </select>

    <select id="findByMap" resultType="com.model.OrderList">
        select
        `id`,`order_no`,`user_id`,`goods`,`username`,`phone`,`start_express`,`end_express`,`arrive_express`,`status`,`create_time`
        from order_list
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
            <if test="orderNo != null and orderNo != ''">
                and order_no = #{orderNo}
            </if>
            <if test="userId != null and userId != ''">
                and user_id = #{userId}
            </if>
            <if test="goods != null and goods != ''">
                and goods = #{goods}
            </if>
            <if test="username != null and username != ''">
                and username = #{username}
            </if>
            <if test="phone != null and phone != ''">
                and phone = #{phone}
            </if>
            <if test="startExpress != null and startExpress != ''">
                and start_express = #{startExpress}
            </if>
            <if test="endExpress != null and endExpress != ''">
                and end_express = #{endExpress}
            </if>
            <if test="arriveExpress != null and arriveExpress != ''">
                and arrive_express = #{arriveExpress}
            </if>
            <if test="status != null and status != ''">
                and status = #{status}
            </if>
            <if test="createTime != null and createTime != ''">
                and create_time = #{createTime}
            </if>
        </where>
    </select>

    <select id="list" resultType="com.model.OrderList">
        select
        `id`,`order_no`,`user_id`,`goods`,`username`,`phone`,`start_express`,`end_express`,`arrive_express`,`status`,`create_time`
        from order_list
        <where>
            <if test="id != null and id != ''">and id =
                #{id}
            </if>
            <if test="orderNo != null and orderNo != ''">and order_no =
                #{orderNo}
            </if>
            <if test="userId != null and userId != ''">and user_id =
                #{userId}
            </if>
            <if test="goods != null and goods != ''">and goods =
                #{goods}
            </if>
            <if test="username != null and username != ''">and username =
                #{username}
            </if>
            <if test="phone != null and phone != ''">and phone =
                #{phone}
            </if>
            <if test="startExpress != null and startExpress != ''">and start_express =
                #{startExpress}
            </if>
            <if test="endExpress != null and endExpress != ''">and end_express =
                #{endExpress}
            </if>
            <if test="arriveExpress != null and arriveExpress != ''">and arrive_express =
                #{arriveExpress}
            </if>
            <if test="status != null and status != ''">and status =
                #{status}
            </if>
            <if test="createTime != null and createTime != ''">and create_time =
                #{createTime}
            </if>
        </where>
    </select>

    <insert id="save" parameterType="com.model.OrderList" useGeneratedKeys="true" keyProperty="id">
        insert into order_list(
        <trim suffixOverrides=",">
            <if test="orderNo != null">`order_no`,</if>
            <if test="userId != null">`user_id`,</if>
            <if test="goods != null">`goods`,</if>
            <if test="username != null">`username`,</if>
            <if test="phone != null">`phone`,</if>
            <if test="startExpress != null">`start_express`,</if>
            <if test="endExpress != null">`end_express`,</if>
            <if test="arriveExpress != null">`arrive_express`,</if>
            <if test="status != null">`status`,</if>
            <if test="createTime != null">`create_time`</if>
        </trim>
        )
        values (
        <trim suffixOverrides=",">
            <if test="orderNo != null">#{orderNo},</if>
            <if test="userId != null">#{userId},</if>
            <if test="goods != null">#{goods},</if>
            <if test="username != null">#{username},</if>
            <if test="phone != null">#{phone},</if>
            <if test="startExpress != null">#{startExpress},</if>
            <if test="endExpress != null">#{endExpress},</if>
            <if test="arriveExpress != null">#{arriveExpress},</if>
            <if test="status != null">#{status},</if>
            <if test="createTime != null">#{createTime}</if>
        </trim>
        )
    </insert>

    <update id="update" parameterType="com.model.OrderList">
        update order_list
        <set>
            <if test="orderNo != null">`order_no` = #{orderNo}
                ,
            </if>
            <if test="userId != null">`user_id` = #{userId}
                ,
            </if>
            <if test="goods != null">`goods` = #{goods}
                ,
            </if>
            <if test="username != null">`username` = #{username}
                ,
            </if>
            <if test="phone != null">`phone` = #{phone}
                ,
            </if>
            <if test="startExpress != null">`start_express` = #{startExpress}
                ,
            </if>
            <if test="endExpress != null">`end_express` = #{endExpress}
                ,
            </if>
            <if test="arriveExpress != null">`arrive_express` = #{arriveExpress}
                ,
            </if>
            <if test="status != null">`status` = #{status}
                ,
            </if>
            <if test="createTime != null">`create_time` = #{createTime}
            </if>
        </set>
        where id = #{id}
    </update>

    <delete id="deleteById">
        delete
        from order_list
        where id = #{id}
    </delete>

</mapper>